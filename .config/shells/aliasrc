 	la="ls -Ah" \
	ls="ls -h" \
	l="ls -lAh" \
	cp="cp -v" \
	rm="rm -v" \
	mv="mv -v" \
 	vim="nvim" \
	..="cd .." \
	...="cd ../.." \
	.3="cd ../../.." \
	.4="cd ../../../../" \
	.5="cd ../../../../.." \
	zshup="source $HOME/.config/shells/zsh/.zshrc" \
	b="cd ~/.local/bin" \
	d="cd ~/Documents" \
	bk="cd ~/Documents/Books" \
	dw="cd ~/Downloads" \
	g="cd ~/Documents/Projects/git" \
	c="cd ~/.config" \
	bc="bc -ls" \
	serv="ssh -Y jmm@jacobmcdonnell.com" \
	sfiles="sftp files@jacobmcdonnell.com" \
	gc="git clone" \
	m="micro" \
	e="cd ~/Documents/Projects/Experiments" \
	v="nvim"
